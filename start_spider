scrapy crawl RGSpider1 -a start_url=http://www.brown.edu -a domain=brown.edu -a university="Brown University"
scrapy crawl RGSpider1 -a start_url=https://www.amherst.edu -a domain=amherst.edu -a university="Amherst College"
scrapy crawl RGSpider1 -a start_url=http://www.mit.edu -a domain=mit.edu -a university="Massachusetts Institute of Technology"
scrapy crawl RGSpider1 -a start_url=http://www.usma.edu/SitePages/Home.aspx -a domain=usma.edu -a university="United States Military Academy"
scrapy crawl RGSpider1 -a start_url=http://www.upenn.edu -a domain=upenn.edu -a university="University of Pennsylvania"
scrapy crawl RGSpider1 -a start_url=http://dartmouth.edu -a domain=dartmouth.edu -a university="Dartmouth College"
scrapy crawl RGSpider1 -a start_url=http://www.columbia.edu -a domain=columbia.edu -a university="Columbia University"
scrapy crawl RGSpider1 -a start_url=http://www.northwestern.edu -a domain=northwestern.edu -a university="Northwestern University"
scrapy crawl RGSpider1 -a start_url=http://www.wesleyan.edu -a domain=wesleyan.edu -a university="Wesleyan University"
scrapy crawl RGSpider1 -a start_url=http://www.cmc.edu -a domain=cmc.edu -a university="Claremont McKenna College"
scrapy crawl RGSpider1 -a start_url=https://www.haverford.edu -a domain=haverford.edu -a university="Haverford College"
scrapy crawl RGSpider1 -a start_url=http://www.uchicago.edu -a domain=uchicago.edu -a university="University of Chicago"
scrapy crawl RGSpider1 -a start_url=http://www.bowdoin.edu -a domain=bowdoin.edu -a university="Bowdoin College"
